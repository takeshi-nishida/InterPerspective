<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
           xmlns:s="library://ns.adobe.com/flex/spark" 
           xmlns:mx="library://ns.adobe.com/flex/mx" width="10" height="50">

    <fx:Declarations>
        <fx:Boolean id="topHead">true</fx:Boolean>
        <fx:Boolean id="bottomHead">true</fx:Boolean>
        <fx:uint id="fillColor">0x000000</fx:uint>
    </fx:Declarations>

    <s:Line top="18" bottom="18" left="4" right="4">
        <s:stroke>
            <s:SolidColorStroke weight="3" color="0x000000" />
        </s:stroke>
    </s:Line>

    <s:Graphic left="0" right="0" top="0" width="10" height="20" visible="{topHead}">
        <s:Path data="M 5 0 L 10 20 L 0 20 Z">
            <s:fill>
                <s:SolidColor color="0x000000" />
            </s:fill>
        </s:Path>
    </s:Graphic>

    <s:Graphic left="0" right="0" bottom="0" width="10" height="20" visible="{bottomHead}">
        <s:Path data="M 0 0 L 10 0 L 5 20 Z">
            <s:fill>
                <s:SolidColor color="0x000000" />
            </s:fill>
        </s:Path>
    </s:Graphic>
    
</s:Group>
